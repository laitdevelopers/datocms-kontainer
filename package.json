{
	"name": "datocms-plugin-kontainer",
	"description": "Pick and choose images from your Kontainer application and save the urls in DatoCMS",
	"version": "1.1.0",
	"homepage": "https://kontainer.com",
	"publisher": "LAITsoeren",
	"author": {
		"name": "LAIT soeren",
		"email": "soeren@lait.dk"
	},
	"icon": "kontainer-logo.png",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/laitdevelopers/datocms-kontainer"
	},
	"keywords": [
		"datocms-plugin"
	],
	"dependencies": {
		"@types/node": "^16.11.58",
		"@types/react": "^18.0.19",
		"@types/react-dom": "^18.0.6",
		"datocms-plugin-sdk": "^0.6.4",
		"datocms-react-ui": "^0.6.4",
		"final-form": "^4.20.7",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-final-form": "^6.5.9",
		"typescript": "^4.8.3"
	},
	"scripts": {
		"start": "cross-env BROWSER='none' PUBLIC_URL='/' react-scripts start",
		"build": "cross-env PUBLIC_URL='.' react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"prepublishOnly": "npm run build"
	},
	"files": [
		"build/",
		"kontainer-logo.png"
	],
	"eslintConfig": {
		"extends": [
			"react-app"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"cross-env": "^7.0.3",
		"react-scripts": "5.0.1"
	},
	"datoCmsPlugin": {
		"title": "Kontainer plugin",
		"coverImage": "kontainer-logo.png",
		"entryPoint": "build/index.html",
		"permissions": []
	}
}
